{
    "commands": {
        "c": "compile",
        "ch": "check",
        "chd": "chdata",
        "com": "command",
        "con": "config",
        "j": "judge",
        "r": "run"
    },
    "compiler": {
        "argu": [
            "-O2",
            "-DLOCAL",
            "-I",
            "%{FILE_PATH}%"
        ],
        "command": "g++",
        "skip_compiled": true,
        "time_limit": 5000,
        "try_times": 5
    },
    "data": {
        "ans_args": [],
        "ansfile": "%*12%",
        "chk_args": [
            "%#infile%",
            "%#outfile%",
            "%#ansfile%"
        ],
        "chk_exit_code": "1",
        "chk_outputs": "on",
        "chkfile": "%*13%",
        "compile_argu": [
            "-std=c++14",
            "-O2",
            "-DJUDGING"
        ],
        "data_dir": "%{RUNNING_PATH}%/.data",
        "data_file": "%.data.data_dir%/%#testcase_name% - %#short_result%/data",
        "in_args": [
            "%#seed%",
            "%#testcase_name%"
        ],
        "infile": "%*10%",
        "out_args": [],
        "outfile": "%*11%",
        "time": 1000
    },
    "exefile": "%#file%",
    "max_process_num": 10,
    "max_thread_num": 100,
    "runtime_limit": 5000,
    "show_file_info": true,
    "~list": {
        "localdata": {
            "data": {
                "data_dir": "./.data"
            }
        }
    }
}